{
  "buildCommand": "npm run build",
  "framework": "nextjs",
  "installCommand": "npm install",
  "cleanUrls": true,
  "trailingSlash": false,
  "crons": [
    {
      "path": "/api/cron/match",
      "schedule": "0 0 * * *"
    },
    {
      "path": "/api/cron/coverage-check",
      "schedule": "0 2 * * *"
    },
    {
      "path": "/api/cron/metrics",
      "schedule": "0 3 * * *"
    },
    {
      "path": "/api/cron/anomaly-detection",
      "schedule": "0 * * * *"
    },
    {
      "path": "/api/cron/email-sequences",
      "schedule": "0 * * * *"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    }
  ]
}
